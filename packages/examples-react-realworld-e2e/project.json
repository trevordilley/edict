{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/examples-react-realworld-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/examples-react-realworld-e2e/cypress.json",
        "devServerTarget": "examples-react-realworld:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "examples-react-realworld:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/examples-react-realworld-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["examples-react-realworld"]
}
